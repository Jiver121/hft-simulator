    "# Set up the trading environment\n",
    "def setup_trading_environment():\n",
    "    \"\"\"Set up portfolio, risk manager, and market making strategy\"\"\"\n",
    "    \n",
    "    # Create portfolio with initial capital\n",
    "    portfolio = Portfolio(initial_cash=100000.0, name=\"Market Making Portfolio\")\n",
    "    \n",
    "    # Create risk manager\n",
    "    risk_manager = RiskManager(initial_capital=100000.0)\n",
    "    \n",
    "    # Configure market making strategy\n",
    "    mm_config = MarketMakingConfig(\n",
    "        spread_target=0.02,      # Target 2 cent spread\n",
    "        position_limit=1000,     # Max 1000 shares position\n",
    "        inventory_target=0,      # Target neutral inventory\n",
    "        risk_aversion=0.1,       # Risk aversion parameter\n",
    "        min_spread=0.01,         # Minimum 1 cent spread\n",
    "        max_spread=0.10,         # Maximum 10 cent spread\n",
    "        quote_size=100,          # Standard 100 share quotes\n",
    "        skew_factor=0.5          # Inventory skewing factor\n",
    "    )\n",
    "    \n",
    "    # Create market making strategy\n",
    "    symbol = \"AAPL\"\n",
    "    mm_strategy = MarketMakingStrategy(\n",
    "        symbols=[symbol],\n",
    "        portfolio=portfolio,\n",
    "        config=mm_config\n",
    "    )\n",
    "    \n",
    "    print(f\"✅ Trading environment setup complete!\")\n",
    "    print(f\"   Portfolio: ${portfolio.initial_cash:,.2f}\")\n",
    "    print(f\"   Symbol: {symbol}\")\n",
    "    print(f\"   Target Spread: {mm_config.spread_target:.3f}\")\n",
    "    print(f\"   Position Limit: {mm_config.position_limit} shares\")\n",
    "    \n",
    "    return portfolio, risk_manager, mm_strategy, symbol\n",
    "\n",
    "# Set up the environment\n",
    "portfolio, risk_manager, mm_strategy, symbol = setup_trading_environment()"
