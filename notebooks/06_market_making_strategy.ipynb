    "# Run the market making strategy backtest\n",
    "def run_market_making_backtest(strategy, market_data, portfolio, risk_manager):\n",
    "    \"\"\"Run comprehensive backtest of market making strategy\"\"\"\n",
    "    \n",
    "    print(\"🔄 Running Market Making Backtest...\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    # Start the strategy\n",
    "    strategy.start()\n",
    "    \n",
    "    # Track results\n",
    "    results = {\n",
    "        'timestamps': [],\n",
    "        'prices': [],\n",
    "        'portfolio_values': [],\n",
    "        'positions': [],\n",
    "        'pnl': [],\n",
    "        'signals_generated': 0,\n",
    "        'trades_executed': 0\n",
    "    }\n",
    "    \n",
    "    # Process each market data point\n",
    "    for i, data_point in enumerate(market_data):\n",
    "        # Generate trading signals\n",
    "        signals = strategy.generate_signals(data_point)\n",
    "        results['signals_generated'] += len(signals)\n",
    "        \n",
    "        # Simulate order execution (simplified)\n",
    "        for signal in signals:\n",
    "            # Simulate execution with some probability\n",
    "            if np.random.random() < 0.1:  # 10% execution probability\n",
    "                # Create simulated trade\n",
    "                trade = Trade(\n",
    "                    trade_id=f\"T{results['trades_executed']}\",\n",
    "                    symbol=signal.symbol,\n",
    "                    volume=signal.quantity,\n",
    "                    price=signal.price,\n",
    "                    timestamp=data_point.timestamp,\n",
    "                    buy_order_id=signal.order_id if signal.side == OrderSide.BUY else None,\n",
    "                    sell_order_id=signal.order_id if signal.side == OrderSide.SELL else None\n",
    "                )\n",
    "                \n",
    "                # Add trade to portfolio\n",
    "                portfolio.add_trade(trade)\n",
    "                results['trades_executed'] += 1\n",
    "        \n",
    "        # Update portfolio with current prices\n",
    "        portfolio.update_prices({symbol: data_point.price}, data_point.timestamp)\n",
    "        \n",
    "        # Update risk manager\n",
    "        risk_manager.update_portfolio_value(portfolio.total_value, data_point.timestamp)\n",
    "        \n",
    "        # Record results\n",
    "        results['timestamps'].append(data_point.timestamp)\n",
    "        results['prices'].append(data_point.price)\n",
    "        results['portfolio_values'].append(portfolio.total_value)\n",
    "        results['positions'].append(strategy.get_position(symbol))\n",
    "        results['pnl'].append(portfolio.total_pnl)\n",
    "        \n",
    "        # Progress update\n",
    "        if (i + 1) % 200 == 0:\n",
    "            print(f\"   Processed {i+1}/{len(market_data)} data points...\")\n",
    "    \n",
    "    print(f\"\\n✅ Backtest completed!\")\n",
    "    print(f\"   Signals generated: {results['signals_generated']}\")\n",
    "    print(f\"   Trades executed: {results['trades_executed']}\")\n",
    "    print(f\"   Final portfolio value: ${portfolio.total_value:,.2f}\")\n",
    "    print(f\"   Total P&L: ${portfolio.total_pnl:,.2f}\")\n",
    "    \n",
    "    return results\n",
