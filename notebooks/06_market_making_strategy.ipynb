    "# First, let's define simplified classes for demonstration\n",
    "from dataclasses import dataclass\n",
    "from enum import Enum\n",
    "from typing import List, Optional\n",
    "import uuid\n",
    "\n",
    "class OrderSide(Enum):\n",
    "    BUY = 'BUY'\n",
    "    SELL = 'SELL'\n",
    "\n",
    "class OrderType(Enum):\n",
    "    MARKET = 'MARKET'\n",
    "    LIMIT = 'LIMIT'\n",
    "\n",
    "@dataclass\n",
    "class Order:\n",
    "    symbol: str\n",
    "    side: OrderSide\n",
    "    quantity: int\n",
    "    price: float\n",
    "    order_type: OrderType\n",
    "    order_id: str = None\n",
    "    \n",
    "    def __post_init__(self):\n",
    "        if self.order_id is None:\n",
    "            self.order_id = str(uuid.uuid4())\n",
    "\n",
    "@dataclass\n",
    "class Trade:\n",
    "    trade_id: str\n",
    "    symbol: str\n",
    "    volume: int\n",
    "    price: float\n",
    "    timestamp: pd.Timestamp\n",
    "    buy_order_id: Optional[str] = None\n",
    "    sell_order_id: Optional[str] = None\n",
    "\n",
    "@dataclass\n",
    "class BookSnapshot:\n",
    "    symbol: str\n",
    "    timestamp: pd.Timestamp\n",
    "    bids: List[tuple]  # [(price, volume), ...]\n",
    "    asks: List[tuple]  # [(price, volume), ...]\n",
    "\n",
    "@dataclass\n",
    "class MarketDataPoint:\n",
    "    symbol: str\n",
    "    timestamp: pd.Timestamp\n",
    "    price: float\n",
    "    volume: int\n",
    "    book_snapshot: BookSnapshot\n",
    "\n",
    "@dataclass\n",
    "class MarketMakingConfig:\n",
    "    spread_target: float\n",
    "    position_limit: int\n",
    "    inventory_target: int\n",
    "    risk_aversion: float\n",
    "    min_spread: float\n",
    "    max_spread: float\n",
    "    quote_size: int\n",
    "    skew_factor: float\n",
    "\n",
    "# Simplified Portfolio class\n",
    "class Portfolio:\n",
    "    def __init__(self, initial_cash: float, name: str):\n",
    "        self.initial_cash = initial_cash\n",
    "        self.name = name\n",
    "        self.cash = initial_cash\n",
    "        self.positions = {}\n",
    "        self.trades = []\n",
    "        self.current_prices = {}\n",
    "        \n",
    "    @property\n",
    "    def total_value(self):\n",
    "        position_value = sum(pos * self.current_prices.get(symbol, 0) \n",
    "                             for symbol, pos in self.positions.items())\n",
    "        return self.cash + position_value\n",
    "    \n",
    "    @property\n",
    "    def total_pnl(self):\n",
    "        return self.total_value - self.initial_cash\n",
    "    \n",
    "    def add_trade(self, trade: Trade):\n",
    "        self.trades.append(trade)\n",
    "        # Update position\n",
    "        if trade.symbol not in self.positions:\n",
    "            self.positions[trade.symbol] = 0\n",
    "        # Determine if buy or sell based on order IDs\n",
    "        if trade.buy_order_id:\n",
    "            self.positions[trade.symbol] += trade.volume\n",
    "            self.cash -= trade.volume * trade.price\n",
    "        else:\n",
    "            self.positions[trade.symbol] -= trade.volume\n",
    "            self.cash += trade.volume * trade.price\n",
    "    \n",
    "    def update_prices(self, prices: dict, timestamp):\n",
    "        self.current_prices.update(prices)\n",
    "    \n",
    "    def calculate_performance_metrics(self):\n",
    "        # Simplified performance metrics\n",
    "        class PerformanceMetrics:\n",
    "            def __init__(self, portfolio):\n",
    "                self.total_return = portfolio.total_pnl / portfolio.initial_cash\n",
    "                self.sharpe_ratio = 1.5  # Simplified\n",
    "                self.max_drawdown = 0.05  # Simplified\n",
    "                self.win_rate = 0.65  # Simplified\n",
    "                self.profit_factor = 1.8  # Simplified\n",
    "        return PerformanceMetrics(self)\n",
    "\n",
    "# Simplified RiskManager class\n",
    "class RiskManager:\n",
    "    def __init__(self, initial_capital: float):\n",
    "        self.initial_capital = initial_capital\n",
    "        self.portfolio_values = []\n",
    "    \n",
    "    def update_portfolio_value(self, value: float, timestamp):\n",
    "        self.portfolio_values.append(value)\n",
    "    \n",
    "    def get_risk_summary(self):\n",
    "        return {\n",
    "            'current_drawdown': 0.02,  # Simplified\n",
    "            'portfolio_volatility': 0.15,  # Simplified\n",
    "            'portfolio_var': 0.05  # Simplified\n",
    "        }\n",
    "\n",
    "# Simplified MarketMakingStrategy class\n",
    "class MarketMakingStrategy:\n",
    "    def __init__(self, symbols: List[str], portfolio: Portfolio, config: MarketMakingConfig):\n",
    "        self.symbols = symbols\n",
    "        self.portfolio = portfolio\n",
    "        self.config = config\n",
    "        self.active = False\n",
    "    \n",
    "    def start(self):\n",
    "        self.active = True\n",
    "    \n",
    "    def get_position(self, symbol: str):\n",
    "        return self.portfolio.positions.get(symbol, 0)\n",
    "    \n",
    "    def generate_signals(self, data_point: MarketDataPoint):\n",
    "        if not self.active:\n",
    "            return []\n",
    "        \n",
    "        signals = []\n",
    "        \n",
    "        # Get current position\n",
    "        position = self.get_position(data_point.symbol)\n",
    "        \n",
    "        # Calculate skew based on inventory\n",
    "        inventory_skew = position * self.config.skew_factor * 0.01\n",
    "        \n",
    "        # Generate bid signal\n",
    "        bid_price = data_point.price - self.config.spread_target/2 - inventory_skew\n",
    "        bid_signal = Order(\n",
    "            symbol=data_point.symbol,\n",
    "            side=OrderSide.BUY,\n",
    "            quantity=self.config.quote_size,\n",
    "            price=bid_price,\n",
    "            order_type=OrderType.LIMIT\n",
    "        )\n",
    "        signals.append(bid_signal)\n",
    "        \n",
    "        # Generate ask signal\n",
    "        ask_price = data_point.price + self.config.spread_target/2 + inventory_skew\n",
    "        ask_signal = Order(\n",
    "            symbol=data_point.symbol,\n",
    "            side=OrderSide.SELL,\n",
    "            quantity=self.config.quote_size,\n",
    "            price=ask_price,\n",
    "            order_type=OrderType.LIMIT\n",
    "        )\n",
    "        signals.append(ask_signal)\n",
    "        \n",
    "        return signals\n",
    "\n",
    "print('📚 Simplified classes defined for demonstration')\n",
    "\n",
    "# Let's create a simple market making simulation to understand the basics\n",
    "def demonstrate_market_making_basics():\n",
    "    \"\"\"Demonstrate basic market making mechanics\"\"\"\n",
    "    \n",
    "    print(\"📊 Market Making Basics Demonstration\")\n",
    "    print(\"=\" * 50)\n",
    "    \n",
    "    # Simulate market making over 100 periods\n",
    "    np.random.seed(42)\n",
    "    periods = 100\n",
    "    \n",
    "    # Market parameters\n",
    "    initial_price = 100.0\n",
    "    spread = 0.04  # 4 cent spread\n",
    "    \n",
    "    # Track results\n",
    "    prices = []\n",
    "    inventory = 0\n",
    "    cash = 0\n",
    "    pnl_history = []\n",
    "    \n",
    "    for period in range(periods):\n",
    "        # Simulate price movement\n",
    "        price_change = np.random.normal(0, 0.01)  # 1% volatility\n",
    "        current_price = initial_price + np.cumsum([price_change])[0]\n",
    "        prices.append(current_price)\n",
    "        \n",
    "        # Market making quotes\n",
    "        bid_price = current_price - spread/2\n",
    "        ask_price = current_price + spread/2\n",
    "        \n",
    "        # Simulate trading (simplified)\n",
    "        trade_probability = 0.3  # 30% chance of trade each period\n",
    "        \n",
    "        if np.random.random() < trade_probability:\n",
    "            # Random trade direction\n",
    "            if np.random.random() < 0.5:\n",
    "                # Someone hits our bid (we buy)\n",
    "                inventory += 100\n",
    "                cash -= bid_price * 100\n",
    "                print(f\"Period {period+1}: BUY 100 @ ${bid_price:.2f}\")\n",
    "            else:\n",
    "                # Someone lifts our offer (we sell)\n",
    "                inventory -= 100\n",
    "                cash += ask_price * 100\n",
    "                print(f\"Period {period+1}: SELL 100 @ ${ask_price:.2f}\")\n",
    "        \n",
    "        # Calculate mark-to-market P&L\n",
    "        mtm_value = inventory * current_price\n",
    "        total_pnl = cash + mtm_value\n",
    "        pnl_history.append(total_pnl)\n",
    "        \n",
    "        initial_price = current_price  # Update for next period\n",
    "    \n",
    "    # Analysis\n",
    "    final_pnl = pnl_history[-1]\n",
    "    max_inventory = max(abs(inv) for inv in [inventory] if isinstance(inv, (int, float)))\n",
    "    \n",
    "    print(f\"\\n📈 Results Summary:\")\n",
    "    print(f\"   Final P&L: ${final_pnl:.2f}\")\n",
    "    print(f\"   Final Inventory: {inventory} shares\")\n",
    "    print(f\"   Final Cash: ${cash:.2f}\")\n",
    "    print(f\"   Average P&L per Period: ${final_pnl/periods:.2f}\")\n",
    "    \n",
    "    # Plot results\n",
    "    fig, axes = plt.subplots(2, 1, figsize=(12, 10))\n",
    "    \n",
    "    # Price evolution\n",
    "    axes[0].plot(prices, color='blue', linewidth=2, label='Price')\n",
    "    axes[0].set_title('Price Evolution')\n",
    "    axes[0].set_ylabel('Price ($)')\n",
    "    axes[0].legend()\n",
    "    axes[0].grid(True, alpha=0.3)\n",
    "    \n",
    "    # P&L evolution\n",
    "    axes[1].plot(pnl_history, color='green', linewidth=2, label='Cumulative P&L')\n",
    "    axes[1].axhline(y=0, color='black', linestyle='--', alpha=0.5)\n",
    "    axes[1].set_title('Market Making P&L')\n",
    "    axes[1].set_xlabel('Period')\n",
    "    axes[1].set_ylabel('P&L ($)')\n",
    "    axes[1].legend()\n",
    "    axes[1].grid(True, alpha=0.3)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "demonstrate_market_making_basics()"
   ]
