    "# Generate realistic market data for backtesting\n",
    "def generate_market_data(symbol, periods=1000, base_price=150.0):\n",
    "    \"\"\"Generate realistic market data for backtesting\"\"\"\n",
    "    \n",
    "    np.random.seed(42)  # For reproducible results\n",
    "    \n",
    "    # Generate timestamps\n",
    "    start_time = pd.Timestamp('2024-01-01 09:30:00')\n",
    "    timestamps = pd.date_range(start_time, periods=periods, freq='1S')\n",
    "    \n",
    "    # Generate price series with realistic characteristics\n",
    "    price_changes = np.random.normal(0, 0.001, periods)  # 0.1% per second volatility\n",
    "    prices = base_price * np.exp(np.cumsum(price_changes))\n",
    "    \n",
    "    # Generate volumes\n",
    "    volumes = np.random.exponential(500, periods).astype(int)\n",
    "    \n",
    "    market_data = []\n",
    "    \n",
    "    for i, (timestamp, price, volume) in enumerate(zip(timestamps, prices, volumes)):\n",
    "        # Create realistic order book snapshot\n",
    "        spread = np.random.uniform(0.01, 0.05)  # 1-5 cent spread\n",
    "        \n",
    "        best_bid = price - spread/2\n",
    "        best_ask = price + spread/2\n",
    "        \n",
    "        # Create book snapshot\n",
    "        book_snapshot = BookSnapshot(\n",
    "            symbol=symbol,\n",
    "            timestamp=timestamp,\n",
    "            bids=[\n",
    "                (best_bid, np.random.randint(100, 1000)),\n",
    "                (best_bid - 0.01, np.random.randint(100, 800)),\n",
    "                (best_bid - 0.02, np.random.randint(100, 600))\n",
    "            ],\n",
    "            asks=[\n",
    "                (best_ask, np.random.randint(100, 1000)),\n",
    "                (best_ask + 0.01, np.random.randint(100, 800)),\n",
    "                (best_ask + 0.02, np.random.randint(100, 600))\n",
    "            ]\n",
    "        )\n",
    "        \n",
    "        # Create market data point\n",
    "        data_point = MarketDataPoint(\n",
    "            symbol=symbol,\n",
    "            timestamp=timestamp,\n",
    "            price=price,\n",
    "            volume=volume,\n",
    "            book_snapshot=book_snapshot\n",
    "        )\n",
    "        \n",
    "        market_data.append(data_point)\n",
    "    \n",
    "    print(f\"📊 Generated {len(market_data)} market data points\")\n",
    "    print(f\"   Time range: {timestamps[0]} to {timestamps[-1]}\")\n",
    "    print(f\"   Price range: ${min(prices):.2f} to ${max(prices):.2f}\")\n",
    "    print(f\"   Average volume: {np.mean(volumes):.0f} shares\")\n",
    "    \n",
    "    return market_data\n",
    "\n",
    "# Generate market data\n",
    "market_data = generate_market_data(symbol, periods=1000)"
   ]
